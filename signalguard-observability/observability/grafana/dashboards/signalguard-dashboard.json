{
  "id": null,
  "title": "SignalGuard Overview",
  "uid": "signalguard-main",
  "schemaVersion": 39,
  "version": 1,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "Requests per endpoint",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(app_requests_total[5m])",
          "legendFormat": "{{endpoint}} {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "graph",
      "title": "Error rate per endpoint",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(app_request_errors_total[5m])",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "graph",
      "title": "Request latency",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(app_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p95 {{endpoint}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "type": "graph",
      "title": "Anomaly score",
      "id": 4,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "signalguard_anomaly_score",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    }
  ]
}
